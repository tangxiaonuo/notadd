type AccessTokenEntity{
	access_token_id: Int! @unique
	"""根据access_token查询用户Openid，添加Index"""
	access_token: String!
	openid: String!
	appid: String!
	ip: String!
	platform: String!
	grant_type: Int!
	scope: String!
	expires_in: DateTime!
	create_time: DateTime!
	update_time: DateTime!
	status: Int!
	"""刷新凭证"""
	refreshToken: RefreshTokenEntity!
}
